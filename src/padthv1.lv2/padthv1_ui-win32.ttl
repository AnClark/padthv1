@prefix lv2:     <http://lv2plug.in/ns/lv2core#> .
@prefix lv2ui:   <http://lv2plug.in/ns/extensions/ui#> .

@prefix padthv1_lv2: <http://padthv1.sourceforge.net/lv2#> .

<http://padthv1.sourceforge.net/lv2>
	lv2ui:ui padthv1_lv2:ui_windows, padthv1_lv2:ui_external .

<http://padthv1.sourceforge.net/lv2#ui>
	a lv2ui:Qt6UI ;
	lv2:requiredFeature <http://lv2plug.in/ns/ext/instance-access> ;
	lv2ui:binary <padthv1.dll> .

<http://padthv1.sourceforge.net/lv2#ui_windows>
	a lv2ui:WindowsUI ;
	lv2:requiredFeature <http://lv2plug.in/ns/ext/instance-access> ;
	lv2:optionalFeature lv2ui:resize, lv2ui:idleInterface, lv2ui:showInterface ;
	lv2:extensionData lv2ui:resize, lv2ui:idleInterface, lv2ui:showInterface ;
	lv2ui:binary <padthv1.dll> .

<http://padthv1.sourceforge.net/lv2#ui_external>
	a <http://kxstudio.sf.net/ns/lv2ext/external-ui#Widget> ;
	lv2:requiredFeature <http://lv2plug.in/ns/ext/instance-access> ;
	lv2ui:binary <padthv1.dll> .
